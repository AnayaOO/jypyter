   "source": [
    "import tkinter as tk\n",
    "\n",
    "calc=tk.Tk()\n",
    "\n",
    "#adding title\n",
    "calc.title(\"calculator\")\n",
    "\n",
    "#specifing size\n",
    "calc.geometry('400x400')\n",
    "\n",
    "#background colour\n",
    "calc.configure(bg='cyan')\n",
    "\n",
    "#transparent background\n",
    "calc.attributes(\"-alpha\",0.9)\n",
    "\n",
    "calc.mainloop()"
   ]
